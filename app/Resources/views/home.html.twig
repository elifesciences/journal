{% extends '::page.html.twig' %}

{% block body %}

    {% if carousel %}

        {{ render_pattern(carousel) }}

    {% endif %}

    <div class="wrapper">

        {{ render_pattern(leadParas) }}

    </div>

    {% if subjects %}

        <div class="wrapper">

            {{ render_pattern(subjectsLink) }}

        </div>

    {% endif %}

    {% embed 'grid/listing-two-column.html.twig' %}

        {% block main %}

            {{ render_pattern(listing) }}

        {% endblock %}

        {% block secondary %}

            {% if magazine %}

                <div class="grid-secondary-column__item">

                    {{ render_pattern(magazine) }}

                </div>

            {% endif %}

        {% endblock %}

    {% endembed %}

    {% if subjects %}

        <div class="wrapper">

            {{ render_pattern(subjects) }}

        </div>

    {% endif %}

{% endblock %}
