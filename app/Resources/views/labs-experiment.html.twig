{% extends '::page.html.twig' %}

{% block title %}{{ experiment.title }} | Labs{% endblock %}

{% block body %}

    {{ render_pattern(contentHeader) }}

    <div class="wrapper">

        {% if leadParas %}
            {{ render_pattern(leadParas) }}
        {% endif %}

        <div class="grid">

            <div class="grid__item medium--six-twelfths push--large--three-twelfths u-pad-top">

                {% for block in blocks %}
                    {{ render_pattern(block) }}
                {% endfor %}

                {{ render_pattern(feedbackForm) }}

            </div>

        </div>

    </div>

{% endblock %}
