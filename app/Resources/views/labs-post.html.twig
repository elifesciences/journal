{% set criticalCss = 'post' %}

{% set hypothesis = true %}

{% extends '::page.html.twig' %}

{% block title %}{{ title|striptags }} | Labs{% endblock %}

{% block body %}

    {{ render_pattern(contentHeader) }}

    <div class="wrapper">
        {% include 'calls-to-action.html.twig' %}
    </div>

    {% if contextualData %}

        <div class="wrapper">

            {{ render_pattern(contextualData) }}

        </div>

    {% endif %}

    {% embed 'grid/content.html.twig' %}

        {% block main %}

            {% for block in blocks %}
                {{ render_pattern(block) }}
            {% endfor %}


        {% endblock %}

    {% endembed %}

{% endblock %}
