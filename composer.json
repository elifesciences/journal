{
    "name": "elife/journal",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\Journal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\Journal\\": "test/"
        }
    },
    "require": {
        "php": "^7.0",
        "bobthecow/mustache-bundle": "^1.2",
        "csa/guzzle-bundle": "^2.2@alpha",
        "cweagans/composer-patches": "^1.5",
        "elife/api-client": "^1.0@dev",
        "elife/api-sdk": "^1.0@dev",
        "elife/patterns": "dev-master",
        "guzzlehttp/promises": "^1.2",
        "justinrainbow/json-schema": "^2.0@dev",
        "ocramius/package-versions": "^1.1",
        "pagerfanta/pagerfanta": "^1.0",
        "phive/twig-extensions-deferred": "^0.3",
        "puli/repository": "^1.0-beta10",
        "puli/symfony-bundle": "^1.0-beta10",
        "puli/twig-extension": "^1.0-beta8",
        "puli/url-generator": "^1.0-beta4",
        "sensio/distribution-bundle": "^5.0",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/swiftmailer-bundle": "^2.4",
        "symfony/symfony": "^3.1",
        "twig/twig": "^1.24",
        "webmozart/json": "^1.3@dev",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "elife/api": "dev-master",
        "elife/api-validator": "^1.0@dev",
        "guzzlehttp/psr7": "^1.3",
        "jarnaiz/behat-junit-formatter": "^1.3",
        "phpspec/prophecy": "^1.6",
        "phpunit/phpunit": "^5.4",
        "symfony/phpunit-bridge": "^3.1"
    },
    "minimum-stability": "beta",
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "patches": {
            "bobthecow/mustache-bundle": {
                "Remove superfluous argument": "https://github.com/bobthecow/BobthecowMustacheBundle/pull/9.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:elifesciences/patterns-php.git"
        }
    ]
}
